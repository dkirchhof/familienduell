{
    "name": "timetracker",
    "sources": [
        {
            "dir": "src/client",
            "subdirs": true
        },
        {
            "dir": "src/server",
            "subdirs": true
        }
    ],
    "package-specs": [
        {
            "module": "es6",
            "in-source": true
        }
    ],
    "bs-dependencies": [
        "@rescript/core",
        "@rescript/react",
        "rescript-struct"
    ],
    "suffix": ".mjs",
    "jsx": {
        "version": 4,
        "mode": "automatic"
    },
    "bsc-flags": [
        "-open RescriptCore"
    ]
}
